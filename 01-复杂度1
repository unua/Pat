#include<stdio.h>

int main(){
	int k;
	while(scanf("%d",&k)!=EOF){
		int thissum,maxsum,tmp;
		thissum=maxsum=0;
		for(int i=0;i<k;i++){
			scanf("%d",&tmp);
			thissum+=tmp;
			if(thissum>maxsum)
				maxsum=thissum;
			else if(thissum<0)
				thissum=0;
		}		
		printf("%d\n",maxsum);
	}
	return 0;
}
